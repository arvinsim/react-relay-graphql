{
  "name": "twitter-clone-react-relay-graphql",
  "version": "1.0.0",
  "description": "A Twitter clone using React, Relay, and GraphQL",
  "main": "index.js",
  "repository": "https://github.com/arvinsim/twitter-clone-react-relay-graphql.git",
  "author": "Arthur Vincent Simon <developarvin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ./node_modules/.bin/babel-node --presets=es2015 -- ./server/index.js",
    "createdb": "./node_modules/.bin/babel-node ./scripts/createdb.js",
    "seeddb": "./node_modules/.bin/babel-node ./scripts/seeddb.js",
    "setupdb": "yarn createdb && yarn seeddb",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint ./server ./tests",
    "lint:fix": "./node_modules/.bin/eslint ./server ./tests --fix"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "faker": "^4.1.0",
    "graphql-tools": "^0.11.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.3",
    "knex": "^0.12.9",
    "sqlite3": "^3.1.8"
  }
}
